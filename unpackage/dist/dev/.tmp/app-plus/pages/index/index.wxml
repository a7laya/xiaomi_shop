<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',[index]]]]]}}" bindtap="__e"><view class="tab-item">{{''+tab.name+''}}</view></view></block></scroll-view><swiper class="swiper-box" current="{{tabIndex}}" duration="300" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{newsitems}}" wx:for-item="tab" wx:for-index="index1" wx:key="index1"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadMore',[index1]]]]]}}" bindscrolltolower="__e"><block wx:if="{{tabBars[index1].template==='index'}}"><block wx:for="{{tab.data}}" wx:for-item="v" wx:for-index="i" wx:key="i"><block><block wx:if="{{v.type==='swiper'}}"><swiper-image vue-id="{{'1-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></swiper-image></block><block wx:if="{{v.type==='indexNavs'}}"><index-nav vue-id="{{'2-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></index-nav><divider vue-id="{{'3-'+index1+'-'+i}}" bind:__l="__l"></divider></block><block wx:if="{{v.type==='threeAdvs'}}"><three-adv vue-id="{{'4-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></three-adv><divider vue-id="{{'5-'+index1+'-'+i}}" bind:__l="__l"></divider></block><block wx:if="{{v.type==='oneAdv'}}"><card vue-id="{{'6-'+index1+'-'+i}}" headTitle="{{v.data.title}}" bodyCover="{{v.data.cover}}" bind:__l="__l"></card></block><block wx:if="{{v.type==='commonList'}}"><view class="row j-sb"><block wx:for="{{v.data}}" wx:for-item="listItem" wx:for-index="listIndex" wx:key="listIndex"><block><common-list vue-id="{{'7-'+index1+'-'+i+'-'+listIndex}}" item="{{listItem}}" index="{{listIndex}}" bind:__l="__l"></common-list></block></block></view></block></block></block></block><block wx:if="{{tabBars[index1].template==='special'}}"><view>其他模板</view></block><view class="uni-tab-bar-loading"><uni-load-more vue-id="{{'8-'+index1}}" loadingType="{{tab.loadingType}}" contentText="{{loadingText}}" bind:__l="__l"></uni-load-more></view></scroll-view></swiper-item></block></swiper></view>